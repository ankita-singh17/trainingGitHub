# dbstorage.properties
project.name=Test Project

storage.names.rest=rest
storage.rest.enabled=true


storage.screenshot.enabled=true
storage.softmode.screenshots.enabled=true
storage.runProperties.isSandbox=true


# pef.properties
browser.selenium.webdriver.factory.class=toolkit.webdriver.WebDriverFactory
browser.selenium.webdriver.factory.method=getDriver
browser.selenium.webdriver.factory.args.aliases=[URL,PROFILE]
browser.selenium.webdriver.factory.args.types=[java.lang.String,java.lang.String]
browser.pool.record.bad.url=false
browser.pool.record.bad.profile=true
browser.pool.record.bad.browser=true
thread-limiter.is-on=true
thread-limiter.max=12
initial-time.is-on=false
initial-time.rounding-unit=d
initial-time.timeshift="+1d"
lock.monitor.locking-timeout=18000000

